<div class="main user-show">
    <div class="container">
    	<h1 class="users-heading">all friends</h1>
        <img class="img-fluid" src="/friend.png">
        <div class="user">  
            <% @current_user.friends.each do |friend| %>
            <div class="posts-index-item">
            	<% u=User.find(friend)%>
                <%= link_to(u.name, "/users/#{u.id}") %>
            </div>
            <% end %>
        </div>
    </div>
</div>